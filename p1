import numpy as np
import pandas as pd
import matplotlib.pyplot as plt
from sklearn.model_selection import train_test_split
x=np.random.randint(low=0, high=100, size=30)
y=np.random.randint(low=10000, high=50000, size=30)
train_x, test_x, train_y, test_y = train_test_split(x,y,test_size=0.2,random_state=2021)
print(train_x)
print(train_y)
print(test_x)
print(test_y)
print(train_x.shape)
print(train_y.shape)
print(test_x.shape)
print(test_y.shape)

